@page
@model BrizaBreath.Pages.Results.CreateModel
@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager

<div id="myModal" class="modal">
    <div class="modal-content">
        <span class="close" id="closeModal">&times;</span>
        <h1>Subscribe for full access and get 3 days trial</h1>
        <div class="flexModal">
            <div class="modalPrices" id="monthlyBox">
                <h2>Monthly</h2>
                <h3>AUD 17.99</h3>
                <p>1 month</p>
                <input type="radio" id="option1" name="myRadioGroup" checked>
            </div>
            <div class="modalPrices" id="yearlyBox">
                <h2>Yearly</h2>
                <h3>AUD 10.99 / month</h3>
                <p>Billed annualy (AUD 131.88)</p>
                <input type="radio" id="option2" name="myRadioGroup">
            </div>
        </div>
        <p>Choose a subscription and gain full access to app`s content:</p>
        <ul>Customizable breathing techniques</ul>
        <ul>Results to track your progress</ul>
        <ul>Lungs expansion training</ul>
        <ul>Briza Program</ul>
        <ul>Welness tips</ul>
        <ul>Mobility & Stretching</ul>
        <ul>Paranayama Techniques</ul>
        <ul>And more...</ul>
        <button type="button" id="subscriptionBtn2">Learn More</button>
        <form method="post" id="subscriptionForm">
            <input id="SUBuserInput" type="hidden" asp-for="BrizaSubscription.UserId" class="form-control" value="@Model.ResultUser" readonly/>
            <input id="SUBDate" type="text" type="hidden" asp-for="BrizaSubscription.StartDate" readonly/>
            <input id="SUBDate2" type="text" type="hidden" asp-for="BrizaSubscription.EndDate" readonly/>                                            
            <input id="priceChoice" value="3" type="hidden" asp-for="BrizaSubscription.PlanId" readonly/>
            <input id="SUBActive" value="0" type="hidden" asp-for="BrizaSubscription.IsActive" readonly/>
            <input type="submit" value="Subscribe Now" id="subscriptionBtn3" class="subscriptionBtn2" readonly/>
        </form>
        <div id="myModal3" class="modal">
            <div class="modal-content">
                <span class="close" id="closeModal3">&times;</span>
                <p>Your membership starts as soon as you set up payment. Your monthly/yearly auto-renew charge will occur on the last day of the current billing period. We will renew your membership for you (unless you have canceled your subscription in the profile page 24 hours before end of billing cycle). Once you become a member you can manage your subscription. </p>
                <p>
                    By clicking on "Subscribe Now", you are agreeing to the <a href="https://brizabreath.com/terms-conditions/" target="_blank">Terms & Conditions</a> and <a href="https://brizabreath.com/privacy-policy" target="_blank">Privacy Policy</a> of this app
                </p>          
            </div>
        </div>
    </div>
</div>
